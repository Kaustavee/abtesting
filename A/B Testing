  {
   "cell_type": "code",
   "execution_count": 22,
   "id": "beeeda86",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-08-21T00:49:39.191801Z",
     "iopub.status.busy": "2024-08-21T00:49:39.191354Z",
